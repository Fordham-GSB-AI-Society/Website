{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "app/api/crypto-sentiment/route.py": {
      "runtime": "python3.11",
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/crypto-sentiment",
      "dest": "/app/api/crypto-sentiment/route.py"
    }
  ]
}